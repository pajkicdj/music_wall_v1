<h1><%= current_user.name %>'s Songs</h1>


<% current_user.musics.each do |music| %>
  <div class="music">
    <h3>
    <a href="/music/<%= music.id %>"><%= music.song_title %> </a>
    </h3>
    <small>
      (by: <%= music.author %>)
    </small>
    <p>
      <a href="<%= music.url %>"><%= music.url %></a>
    </p>
  </div>
<% end %>
